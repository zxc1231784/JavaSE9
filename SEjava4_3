
public class SEjava4_3 {

	public static void main(String[] args) {
		int number[] = { 70, 80, 31, 37, 10, 1, 48, 60, 33, 80 };
		int min;
		for (int i = 0; i < number.length; i++) {
			for (int j = 0; j <= i; j++) {
				if(number[i]<number[j]) {
					min=number[i];
					number[i]=number[j];
					number[j]=min;
				}
			}
		}
		for(int i=0;i<number.length;i++) {
			System.out.print(number[i]+"\t");
		}
	}

}
